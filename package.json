{
  "name": "nodejs-snowflake",
  "version": "1.2.1",
  "description": "Generate time sortable 64 bits unique ids for distributed systems (inspired from twitter snowflake)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "gypfile": true,
  "keywords": [
    "snowflake",
    "id",
    "generator",
    "unique",
    "node.js",
    "nodejs",
    "twitter",
    "n-api"
  ],
  "scripts": {
    "build": "node-gyp rebuild && tsc",
    "build:napi": "node-gyp rebuild",
    "build:tsc": "tsc",
    "clean": "node-gyp clean",
    "prepare": "npm run build",
    "benchmark": "npm run build:tsc && node lib/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/utkarsh-pro/node-snowflake"
  },
  "files": [
    "lib/**/*",
    "cppsrc/**/*",
    "binding.gyp"
  ],
  "author": "Utkarsh Srivastava",
  "license": "ISC",
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.8",
    "minimist": "^1.2.0",
    "nanotimer": "^0.3.15",
    "node-gyp": "^6.0.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "node-addon-api": "^1.7.1"
  }
}